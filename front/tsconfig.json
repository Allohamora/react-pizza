{
    "compilerOptions": {
        "module": "esnext",
        "target": "es5",
        "jsx": "react",
        "sourceMap": true,
        "baseUrl": "./src",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowJs": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}